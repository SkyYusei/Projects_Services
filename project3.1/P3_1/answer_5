scan 'songdata', {COLUMNS => ['data:artist_name', 'data:title', 'data:year'], FILTER => "SingleColumnValueFilter('data', 'artist_name', = , 'regexstring:Gwen Guthrie.*') AND SingleColumnValueFilter('data', 'title', = , 'regexstring:.*Love.*') AND SingleColumnValueFilter('data', 'title', != , 'regexstring:.*Bitter.*') AND SingleColumnValueFilter('data', 'title', != , 'regexstring:.*Never.*') AND SingleColumnValueFilter('data', 'year', = , 'binary:1990')"}